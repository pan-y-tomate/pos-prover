{
  "name": "ts-merkle-sum-tree",
  "version": "0.0.4",
  "description": "APIs to create efficient merkle sum tree and to generate and verify proofs of membership",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "big-integer": "^1.6.51",
    "circomlibjs": "^0.0.8"
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pan-y-tomate/ts-merkle-sum-tree.git"
  },
  "author": "Enrico Bottazzi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pan-y-tomate/ts-merkle-sum-tree/issues"
  },
  "homepage": "https://github.com/pan-y-tomate/ts-merkle-sum-tree#readme"
}
